{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.md": "${capture}.*.md",
    ".gitattributes": ".gitignore",
    ".markdownlint.*": ".markdownlint-cli2.*",
    "Brewfile": "min.Brewfile, Brewfile.lock.json",
    "cspell.config.yml": ".cspell.cache"
  },
  "files.associations": {
    "*Brewfile": "ruby",
    "LICENSE": "plaintext"
  },
  "files.watcherExclude": {
    "**/.vagrant": true,
    ".cspell.cache": true,
    "Brewfile.lock.json": true
  },
  "git.branchProtection": ["master"],
  "search.exclude": {
    "**/.vagrant": true,
    ".cspell.cache": true,
    "Brewfile.lock.json": true
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json": [
      "cspell.config.yml"
    ],
    "https://storage.googleapis.com/coderabbit_public_assets/schema.v2.json": [
      ".coderabbit.yaml"
    ]
  }
}
